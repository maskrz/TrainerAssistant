<tiles-definitions>
	
	<!-- Registration -->
	<definition name="/registration/registration" extends="base.definition">
		<put-attribute name="title" value="TA | Registration"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/template/header.jsp"></put-attribute>
		<put-attribute name="body" value="/WEB-INF/views/registration/registration.jsp"></put-attribute>
		<put-list-attribute name="javascripts" inherit="true">        	
        	<add-attribute value="https://raw.githubusercontent.com/gdi2290/angular-md5/master/angular-md5.js" />
        	<add-attribute value="/resources/js/registration/registrationController.js" />
        	<add-attribute value="/resources/js/registration/registrationService.js" />        	
    	</put-list-attribute>
	</definition>
	
	<definition name="/profile/profile" extends="base.definition">
		<put-attribute name="title" value="TA | Profile"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/template/header.jsp"></put-attribute>
		<put-attribute name="body" value="/WEB-INF/views/profile/profile.jsp"></put-attribute>
		<put-list-attribute name="javascripts" inherit="true">               	
        	<add-attribute value="https://www.gstatic.com/charts/loader.js" />
        	<add-attribute value="/resources/js/profile/profileController.js" />
        	<add-attribute value="/resources/js/profile/profileService.js" />        	
    	</put-list-attribute>
    	<put-list-attribute name="stylesheets" inherit="true">
        	<add-attribute value="/resources/css/profile/profile.css" />  	
    	</put-list-attribute>
	</definition>


